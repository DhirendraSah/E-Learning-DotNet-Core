@model IEnumerable<EnrollmentViewModel>

@{
    Layout = "~/Views/Shared/_EducatorLayout.cshtml";
}
<link href="~/Content/bootstrap.min.css" rel="stylesheet" />
<link rel="stylesheet" href="~/css/studentEnrolled.css" asp-append-version="true" />
<!-- Add Font Awesome CDN if not already included in your layout -->
<link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.0.0-beta3/css/all.min.css" integrity="sha512-Sf3etsjbzF7GPlZp9YDEeYV1ivhrBtnqXrkW1mpxUgg1xSJu1EiE6vS8Mgo5p0C4KIqLirqKj8zhCBbZoyZtbA==" crossorigin="anonymous" referrerpolicy="no-referrer" />

<h2>Enrolled Students</h2>
<table class="table">
    <thead>
        <tr>
            <th>Student Email</th>
            <th>Course Title</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var enrollment in Model)
        {
            <tr>
                <td>@enrollment.StudentEmail</td>
                <td>@enrollment.CourseTitle</td>
            </tr>
        }
    </tbody>
</table>
